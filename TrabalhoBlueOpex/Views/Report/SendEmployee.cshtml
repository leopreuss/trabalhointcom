@model Employee;

@{
    ViewData["Title"] = "SendEmployee";
}

<h2>Enviar Empregado</h2>

<form asp-controller="Report" asp-action="Create">
    <div class="form-group">
        <input asp-for="Name" class="form-control" />
    </div>
    <div class="form-group">
        <input asp-for="Charge" class="form-control" />
    </div>
    <div class="form-group">
        <input asp-for="Cpf" class="form-control" />
    </div>
    <div class="form-group">
        <input asp-for="DateOfAdmission" class="form-control" />
    </div>
    <div class="form-group">
        <input asp-for="BirthDate" class="form-control" />
    </div>
    <div class="form-group">
        <input asp-for="Identifier" class="form-control" />
    </div>
    <div class="form-group">
        <input asp-for="Passport" class="form-control" />
    </div>
    <div class="form-group">
        <input asp-for="ProfileType" class="form-control" />
    </div>

    <input type="submit" value="Enviar" />
</form>

